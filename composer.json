{
    "name": "http-connect/standard",
    "description": "HttpConnect Standard Interfaces",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Stevan Pavlovic",
            "email": "pavlovicmstevan@gmail.com",
            "homepage": "https://stevanpavlovic.github.io",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "psr/container": "^1.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/log": "^1.1",
        "symfony/validator": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "HttpConnect\\Standard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HttpConnect\\Standard\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": "dist"
    }
}
